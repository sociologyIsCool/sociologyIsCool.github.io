{
  "name": "phaser-state-transition",
  "version": "2.4.3",
  "description": "Phaser state transition plugin",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:cristianbote/phaser-state-transition.git"
  },
  "files": [
    "index.js",
    "dist/phaser-state-transition.js",
    "dist/phaser-state-transition.min.js",
    "README.md",
    "LICENSE.md"
  ],
  "keywords": [
    "phaser",
    "state",
    "transition"
  ],
  "scripts": {
